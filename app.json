{
  "expo": {
    "name": "RodizioPizza",
    "description": "By. SyLu4N",
    "slug": "rodiziopizza-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/imgs/capa.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/gif/spinnerPizza.gif",
        "backgroundColor": "#fe5949"
      },
      "package": "com.sylu4n.rodiziopizzaapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/gif/spinnerPizza.gif",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#fe5949"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f82a924c-3c8e-4367-a92d-aa5187de46b4"
      }
    }
  }
}
